{
  "name": "@sumup/foundry",
  "version": "0.0.0-semantically-released",
  "description": "A toolkit for JavaScript applications by SumUp.",
  "main": "index.js",
  "repository": "github:sumup/foundry",
  "author": "Felix Jung <felix.jung@sumup.com>",
  "license": "Apache-2.0",
  "bin": "cli",
  "scripts": {
    "build": "yarn cleanup && yarn copy && tsc && chmod +x dist/cli/index.js",
    "dev": "yarn cleanup && yarn copy && tsc --watch",
    "start": "yarn dev",
    "cleanup": "rimraf ./dist && mkdir dist",
    "copy": "cp -r src/configs/plop/templates dist/plop-templates && cp package.json dist/package.json && cp LICENSE dist/LICENSE && cp README.md dist/README.md",
    "prelint": "yarn build",
    "lint": "yarn eslint src",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --runInBand --coverage --reporters default --reporters \"jest-junit\"",
    "codedov": "codecov",
    "check:security": "audit-ci --critical",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "prerelease": "yarn build",
    "release": "cd ./dist && semantic-release"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@semantic-release/commit-analyzer": "^7.0.0-beta",
    "@semantic-release/github": "^5.6.0-beta",
    "@semantic-release/npm": "^6.0.0-beta",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "babel-eslint": "^10.0.1",
    "chalk": "^3.0.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.0.0",
    "eslint-plugin-emotion": "^10.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-notice": "^0.8.9",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^2.0.0",
    "husky": "^3.0.0",
    "inquirer": "^7.0.1",
    "lint-staged": "^10.0.0-beta",
    "lodash": "^4.17.11",
    "plop": "^2.1.0",
    "prettier": "^1.15.2",
    "semantic-release": "^16.0.0-beta.18",
    "webpack-merge": "^4.1.4",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.0.0",
    "@types/yargs": "^13.0.3",
    "audit-ci": "^2.0.1",
    "codecov": "^3.1.0",
    "jest": "^24.0.0",
    "jest-extended": "^0.11.2",
    "jest-junit": "^9.0.0",
    "license-checker": "^25.0.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
  }
}
