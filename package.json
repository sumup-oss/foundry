{
  "name": "@sumup/foundry",
  "version": "0.0.0-semantically-released",
  "description": "A toolkit for JavaScript + TypeScript applications by SumUp.",
  "main": "index.js",
  "repository": "https://github.com/sumup-oss/foundry",
  "author": "Felix Jung <felix.jung@sumup.com>, Connor BÃ¤r <connor.baer@sumup.com>",
  "license": "Apache-2.0",
  "bin": "cli/index.js",
  "scripts": {
    "build": "yarn cleanup && yarn copy && tsc && chmod +x dist/cli/index.js",
    "dev": "yarn cleanup && yarn copy && tsc --watch",
    "start": "yarn dev",
    "cleanup": "rimraf ./dist && mkdir dist",
    "copy": "mkdir -p dist/configs/plop/templates && cp -r src/configs/plop/templates dist/configs/plop && cp package.json dist/package.json && cp LICENSE dist/LICENSE && cp README.md dist/README.md",
    "prelint": "yarn build",
    "lint": "eslint src --ext .js,.jsx,.json,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "lint:ci": "yarn lint --format github",
    "test": "jest --watch",
    "test:ci": "jest --ci --runInBand --coverage --reporters default --reporters \"jest-github-reporter\"",
    "codecov": "codecov",
    "check:security": "audit-ci --critical",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "prerelease": "yarn build",
    "release": "cd ./dist && semantic-release"
  },
  "engines": {
    "node": ">=16 || ^14.17"
  },
  "browserslist": [
    "chrome >= 57",
    "firefox >= 54",
    "safari >= 10.3",
    "edge >= 16",
    "opera >= 44",
    "samsung >= 6.2",
    "supports css-grid and supports object-entries"
  ],
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.14.3",
    "@emotion/eslint-plugin": "^11.7.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "chalk": "^4.0.0",
    "cross-spawn": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-github": "^1.0.11",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "handlebars": "^4.7.3",
    "husky": "^4.0.0",
    "inquirer": "^8.2.1",
    "is-ci": "^3.0.0",
    "lint-staged": "^12.3.7",
    "listr": "^0.14.3",
    "listr-inquirer": "^0.1.0",
    "lodash": "^4.17.11",
    "pkg-up": "^3.1.0",
    "plop": "^2.1.0",
    "prettier": "^2.6.0",
    "semantic-release": "^19.0.2",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/dedent": "^0.7.0",
    "@types/inquirer": "^8.1.1",
    "@types/is-ci": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.180",
    "@types/node": "^16.4.13",
    "@types/prettier": "^2.0.0",
    "@types/yargs": "^17.0.10",
    "audit-ci": "^6.1.0",
    "codecov": "^3.1.0",
    "jest": "^27.0.1",
    "jest-github-reporter": "^1.0.2",
    "license-checker": "^25.0.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^27.0.0",
    "typescript": "^4.6.2"
  }
}
