{
  "name": "@sumup/foundry",
  "version": "0.0.0-semantically-released",
  "description": "A toolkit for JavaScript applications by SumUp.",
  "main": "index.js",
  "repository": "github:sumup/foundry",
  "author": "Felix Jung <felix.jung@sumup.com>",
  "license": "Apache-2.0",
  "bin": "cli",
  "scripts": {
    "cleanup": "rimraf ./dist",
    "build": "yarn cleanup && BABEL_ENV=build NODE_ENV=production rollup --config && chmod +x dist/cli",
    "lint": "yarn eslint src",
    "test": "jest",
    "test:watch": "yarn test --watchAll",
    "develop": "BABEL_ENV=develop NODE_ENV=develop rollup --config --watch",
    "codedov": "codecov",
    "start": "yarn cleanup && yarn develop",
    "check:security": "audit-ci --critical",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "prerelease": "yarn build",
    "release": "cd ./dist && semantic-release"
  },
  "files": [
    "babel.js",
    "eslint.js",
    "plop.js",
    "prettier.js",
    "lint-staged.js",
    "husky.js",
    "semantic-release.js",
    "plop-templates",
    "cli",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.0.0",
    "eslint-plugin-emotion": "^10.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-notice": "^0.8.9",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^2.0.0",
    "husky": "^1.2.0",
    "jest": "^24.0.0",
    "jest-extended": "^0.11.2",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "plop": "^2.1.0",
    "prettier": "^1.15.2",
    "semantic-release": "^16.0.0-beta.18",
    "webpack-merge": "^4.1.4",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "audit-ci": "^2.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "esm": "^3.2.25",
    "jest-junit": "^9.0.0",
    "license-checker": "^25.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.13.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-copy": "^2.0.1",
    "rollup-plugin-license": "^0.8.1",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-preserve-shebang": "^0.1.6"
  }
}
