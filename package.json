{
  "name": "@sumup/foundry",
  "version": "0.0.0-semantically-released",
  "description": "A toolkit for JavaScript + TypeScript applications by SumUp.",
  "main": "index.js",
  "repository": "https://github.com/sumup-oss/foundry",
  "author": "Felix Jung <felix.jung@sumup.com>, Connor BÃ¤r <connor.baer@sumup.com>",
  "license": "Apache-2.0",
  "bin": "cli/index.js",
  "scripts": {
    "build": "yarn cleanup && yarn copy && tsc && chmod +x dist/cli/index.js",
    "dev": "yarn cleanup && yarn copy && tsc --watch",
    "start": "yarn dev",
    "cleanup": "rimraf ./dist && mkdir dist",
    "copy": "mkdir -p dist/configs/plop/templates && cp -r src/configs/plop/templates dist/configs/plop && cp package.json dist/package.json && cp LICENSE dist/LICENSE && cp README.md dist/README.md",
    "prelint": "yarn build",
    "lint": "eslint src --ext .js,.jsx,.json,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "lint:ci": "yarn lint --format junit -o __reports__/junit/eslint-results.xml",
    "test": "jest --watch",
    "test:ci": "jest --ci --runInBand --coverage --reporters default --reporters \"jest-junit\"",
    "codedov": "codecov",
    "check:security": "audit-ci --critical",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "prerelease": "yarn build",
    "release": "cd ./dist && semantic-release"
  },
  "engines": {
    "node": ">=10.18.0"
  },
  "dependencies": {
    "@semantic-release/commit-analyzer": "^8.0.0",
    "@semantic-release/github": "^7.0.0",
    "@semantic-release/npm": "^7.0.0",
    "@semantic-release/release-notes-generator": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^4.0.0",
    "cross-spawn": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-emotion": "^10.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.0.0",
    "handlebars": "^4.7.3",
    "husky": "^4.0.0",
    "inquirer": "^7.0.1",
    "lint-staged": "^10.0.0",
    "listr": "^0.14.3",
    "listr-inquirer": "^0.1.0",
    "lodash": "^4.17.11",
    "pkg-up": "^3.1.0",
    "plop": "^2.1.0",
    "prettier": "^2.0.0",
    "semantic-release": "^17.0.0",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/dedent": "^0.7.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.22",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^14.14.37",
    "@types/prettier": "^2.0.0",
    "@types/yargs": "^16.0.1",
    "audit-ci": "^3.0.0",
    "codecov": "^3.1.0",
    "jest": "^26.0.0",
    "jest-junit": "^12.0.0",
    "license-checker": "^25.0.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.1.2"
  }
}
